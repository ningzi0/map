/* pages/my/my.wxss */
.header{
  position: relative;
  height: 600rpx;
}
.header image{
  width: 100%;
  display: block;
  height: 100%;
}
.replace{
  position: absolute;
  bottom: 40rpx;
  left: 40rpx;
}
.user{
  position: absolute;
  right: 80rpx;
  bottom: 0;
  transform: translateY(60%);
}
.userText{
  font-weight: bold;
  text-align: center;
  margin: 10rpx 0;
}
.user image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  display: block;
}

.userContent{
  background-color: #fff;
  margin: 180rpx 40rpx 0;
  border-radius: 15rpx;
}
.userContent navigator{
  display: flex;
  align-items: center;
  padding: 30rpx;
  justify-content: space-between;
  border-bottom: 1px solid #eee;
}
.userContent navigator:last-child{
  border-bottom: 0;
}
.userContent navigator .left{
  display: flex;
  align-items: center;
}
.userContent navigator .left text{
  margin-left: 20rpx;
}